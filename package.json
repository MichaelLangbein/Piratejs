{
  "name": "piratejs",
  "version": "0.0.1",
  "description": "A simple pirate game based on sockets and nodejs. ",
  "main": "index.js",
  "scripts": {
	"all": "npm run build && npm run serve",
    	"serve": "node src/server/index.js",
    	"build": "npm run brwfy",
    	"brwfy": "browserify src/client/client.js -o build/script.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelLangbein/piratejs.git"
  },
  "author": "Michael Langbein",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MichaelLangbein/piratejs/issues"
  },
  "homepage": "https://github.com/MichaelLangbein/piratejs#readme",
  "dependencies": {
    "express": "^4.16.3",
    "p5": "^0.6.0",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0"
  },
  "devDependencies": {
    "browserify": "^16.2.0"
  }
}
